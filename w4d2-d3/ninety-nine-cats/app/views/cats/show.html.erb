<h1><%=@cat.name%></h1>

<head>
  <style>
  table, th, td {
      border: 5px solid black; border-spacing: 2px;
  }
  </style>
  
  </head>

<table>
  <tr>
    <th>Age</th>
    <th>Color</th> 
    <th>Sex</th>
  </tr>
  <tr>
    <td><%=@cat.age%></td> 
    <td><%=@cat.color%></td>
    <td><%=@cat.sex%></td>
  </tr>
</table>
