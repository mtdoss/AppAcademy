<%@cat.cat_rental_requests.each do |request|%>
<li><%="From #{request.start_date} to #{request.end_date}"%></li>
    <%= button_to "Approve", approve_cat_rental_request_url%>
     <%= button_to "Deny", deny_cat_rental_request_url%>
<%end%>
<br>
</ol>




